    "main": "dist/index.esm.cjs",
    "module": "dist/index.esm.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "rimraf dist && nr prepareImports && rollup --config",
        "dev": "rollup --config --watch",
        "prepublishOnly": "nr build",
        "release": "bumpp && npm publish",
        "start": "tsx src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "prepareImports": "tsx ./scripts/prepareImports.ts && pnpm exec prettier --write ./src/index.ts",
        "prepareDocs": "tsx ./scripts/addDocsToReadme.ts && pnpm exec prettier --write ./README.md"
    },
    "devDependencies": {
        "@antfu/ni": "^0.19.0",
        "@types/node": "^18.15.0",
        "bumpp": "^8.2.1",
        "current-week-number": "^1.0.7",
        "happy-dom": "^8.9.0",
        "pnpm": "^7.29.1",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "tsx": "^4.1.4",
        "typescript": "^5.2.2"
    }
}
