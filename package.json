{
    "name": "@banjoanton/utils",
    "type": "module",
    "version": "0.1.30",
    "description": "",
    "author": "Anton Ã–dman <anton.odman@gmail.com>",
    "license": "MIT",
    "private": false,
    "homepage": "https://github.com/banjo/utils#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/banjo/utils.git"
    },
    "bugs": "https://github.com/banjo/utils/issues",
    "keywords": [],
    "main": "dist/index.esm.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.esm.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "rimraf dist && nr prepareImports && rollup --config",
        "dev": "rollup --config --watch",
        "prepublishOnly": "nr build",
        "format": "npx prettier --write .",
        "release": "bumpp && npm publish",
        "start": "esno src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "prepareImports": "esno ./scripts/prepareImports.ts && npx prettier --write ./src/index.ts",
        "prepareDocs": "esno ./scripts/addDocsToReadme.ts && npx prettier --write ./README.md",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@antfu/ni": "^0.19.0",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/node": "^18.11.18",
        "@types/throttle-debounce": "^5.0.0",
        "bumpp": "^8.2.1",
        "dot-prop": "^7.2.0",
        "esbuild": "^0.17.5",
        "esno": "^0.16.3",
        "fast-deep-equal": "^3.1.3",
        "happy-dom": "^8.1.5",
        "husky": "^8.0.0",
        "pnpm": "^7.25.1",
        "prettier": "^2.8.3",
        "pretty-quick": "^3.1.3",
        "rimraf": "^4.1.2",
        "rollup": "^3.10.1",
        "rollup-plugin-dts": "^5.1.1",
        "rollup-plugin-esbuild": "^5.0.0",
        "throttle-debounce": "^5.0.0",
        "toastler": "^0.0.3",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.27.3",
        "wildcard-match": "^5.1.2"
    }
}
