{
    "name": "@banjoanton/utils",
    "type": "module",
    "version": "0.1.17",
    "packageManager": "pnpm@7.12.0",
    "description": "",
    "author": "Anton Ã–dman <anton.odman@gmail.com>",
    "license": "MIT",
    "private": false,
    "homepage": "https://github.com/banjo/utils#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/banjo/utils.git"
    },
    "bugs": "https://github.com/banjo/utils/issues",
    "keywords": [],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "nr prepareImports && rollup --config && nr cleanAfterBuild",
        "dev": "rollup --config --watch",
        "prepublishOnly": "nr build",
        "format": "npx prettier --write .",
        "release": "bumpp && npm publish",
        "start": "esno src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "prepareImports": "esno ./scripts/prepareImports.ts && npx prettier --write ./src/index.ts",
        "prepareDocs": "esno ./scripts/addDocsToReadme.ts && npx prettier --write ./README.md",
        "cleanAfterBuild": "rm dist/index.d.cts && mv dist/index.d.mts dist/index.d.ts",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@antfu/ni": "^0.19.0",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.4.0",
        "@types/node": "^18.11.18",
        "@types/parse-json": "^4.0.0",
        "@types/throttle-debounce": "^5.0.0",
        "bumpp": "^8.2.1",
        "change-case": "^4.1.2",
        "dot-prop": "^7.2.0",
        "esno": "^0.16.3",
        "happy-dom": "^8.1.5",
        "husky": "^8.0.0",
        "pnpm": "^7.25.1",
        "prettier": "^2.8.3",
        "pretty-quick": "^3.1.3",
        "rollup": "^3.10.1",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-ts": "^3.1.1",
        "throttle-debounce": "^5.0.0",
        "toastler": "^0.0.3",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.27.3"
    }
}
